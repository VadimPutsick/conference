<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <decisions>
        <name>Sessions_time_is_overl</name>
        <label>Sessions time is overlap</label>
        <locationX>760</locationX>
        <locationY>99</locationY>
        <defaultConnector>
            <targetReference>Overlap</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Time_is_overlap</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>existedSession.Session_Start__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>End_Session</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>existedSession.Session_End__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>Sart_Session</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Loop_All_Sessions</targetReference>
            </connector>
            <label>Time is not overlap</label>
        </rules>
    </decisions>
    <dynamicChoiceSets>
        <name>conferenceSet</name>
        <dataType>String</dataType>
        <displayField>Name</displayField>
        <object>Conference__c</object>
        <outputAssignments>
            <assignToReference>newSession.Conference__c</assignToReference>
            <field>Id</field>
        </outputAssignments>
        <valueField>Name</valueField>
    </dynamicChoiceSets>
    <dynamicChoiceSets>
        <name>topicTechnologiesSet</name>
        <dataType>Multipicklist</dataType>
