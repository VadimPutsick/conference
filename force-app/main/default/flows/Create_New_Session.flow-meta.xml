        <object xsi:nil="true"/>
        <picklistField>Topic_Technologies__c</picklistField>
        <picklistObject>Session__c</picklistObject>
    </dynamicChoiceSets>
    <interviewLabel>Create New Session {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Create New Session</label>
    <loops>
        <name>Loop_All_Sessions</name>
        <label>Loop All Sessions</label>
        <locationX>498</locationX>
        <locationY>80</locationY>
        <assignNextValueToReference>existedSession</assignNextValueToReference>
        <collectionReference>existedSessionList</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Sessions_time_is_overl</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Create_new_Session</targetReference>
        </noMoreValuesConnector>
    </loops>
