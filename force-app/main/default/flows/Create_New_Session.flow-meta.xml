        <inputAssignments>
            <field>Topic_Technologies__c</field>
            <value>
                <elementReference>Topic_Technologies</elementReference>
            </value>
        </inputAssignments>
        <object>Session__c</object>
    </recordCreates>
    <recordLookups>
        <name>Get_Sessions</name>
        <label>Get Sessions</label>
        <locationX>292</locationX>
        <locationY>52</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Loop_All_Sessions</targetReference>
        </connector>
        <filters>
            <field>Conference__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>newSession.Conference__c</elementReference>
            </value>
        </filters>
        <object>Session__c</object>
        <outputReference>existedSessionList</outputReference>
        <queriedFields>Id</queriedFields>
